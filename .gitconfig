[user]
	name = Zhiyang Fu
	email = zyfu@email.arizona.edu
[core]
	editor = vim
[push] 
	default = matching
[color] 
	ui = auto
[diff]
	tool = vimdiff
[alias]
	lg1 = log --graph --abbrev-commit --decorate --date=relative --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(bold white)- %an%C(reset)%C(bold magenta)%d%C(reset)' --all
	lg2 = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold magenta)%d%C(reset)%n''          %C(white)%s%C(reset) %C(bold white)- %an%C(reset)' --all
lg = !"git lg1"
[merge]
	tool = kdiff3
	log = true
[rerere]
	enabled = 1
[help]
	autocorrect = 0
